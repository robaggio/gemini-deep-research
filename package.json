{
  "name": "gemini-research-agent",
  "version": "1.0.0",
  "description": "A full-featured Gemini Deep Research agent using the Gemini Interactions API",
  "type": "module",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "bin": {
    "gemini-research": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "mkdir -p dist/web/public && cp -r src/web/public/* dist/web/public/",
    "dev": "tsc --watch",
    "start": "node dist/web/server.js",
    "cli": "node dist/cli/index.js",
    "web": "node dist/web/server.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "gemini",
    "google",
    "ai",
    "deep-research",
    "interactions-api",
    "llm",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "marked": "^14.1.0",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "ora": "^8.1.0",
    "simple-git": "^3.30.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.9.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
